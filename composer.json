{
  "name": "drupol/drupal7_session_services",
  "description": "Provides Session services to connect to Drupal 7 session management.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pol Dellaiera",
      "email": "pol.dellaiera@protonmail.com"
    }
  ],
  "require": {
    "php" : "^5.6 || ^7.0 || ^7.1"
  },
  "require-dev": {
    "phpro/grumphp": "^0.12",
    "squizlabs/php_codesniffer": "^3.0",
    "phpmd/phpmd": "^2.6",
    "sebastian/phpcpd": "^3.0",
    "sensiolabs/security-checker": "^4.1",
    "friendsofphp/php-cs-fixer": "^2.7"
  },
  "autoload": {
    "psr-4": {
      "drupol\\drupal7_session_services\\": "src/"
    }
  },
  "scripts": {
    "phpspec": "./vendor/bin/phpspec run",
    "grumphp": "./vendor/bin/grumphp run"
  }
}
